     <relativePath>../..</relativePath>
  </parent>
  <artifactId>commons</artifactId>
  <packaging>pom</packaging>
  <name>${project.artifactId} OfBiz Component</name>
  <url>http://maven.apache.org</url>
  
  <dependencies>

