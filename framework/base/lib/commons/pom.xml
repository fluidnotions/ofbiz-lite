			<overWrite>true</overWrite>
			<outputDirectory>framework/base/lib/commons/</outputDirectory>
		</artifactItem>
		<!-- Ant dependency location:framework/base/lib/commons/commons-digester-1.8.jar-->
		<artifactItem>
			<groupId>commons-digester</groupId>
			<artifactId>commons-digester</artifactId>
			<version>1.8</version>
			<type>jar</type>
			<overWrite>true</overWrite>
			<outputDirectory>framework/base/lib/commons/</outputDirectory>
		</artifactItem>
		<!-- Ant dependency location:framework/base/lib/commons/commons-beanutils-1.7.0.jar-->
		<artifactItem>
			<groupId>commons-beanutils</groupId>
			<artifactId>commons-beanutils</artifactId>
			<version>1.7.0</version>
			<type>jar</type>
			<overWrite>true</overWrite>
			<outputDirectory>framework/base/lib/commons/</outputDirectory>
		</artifactItem>
		<!-- Ant dependency location:framework/base/lib/commons/commons-collections-3.2.jar-->
		<artifactItem>
			<groupId>commons-collections</groupId>
			<artifactId>commons-collections</artifactId>
			<version>3.2</version>
			<type>jar</type>
			<overWrite>true</overWrite>
			<outputDirectory>framework/base/lib/commons/</outputDirectory>
		</artifactItem>
		<!-- Ant dependency location:framework/base/lib/commons/commons-pool-1.3.jar-->
